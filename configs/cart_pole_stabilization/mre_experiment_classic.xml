<mre_experiment>
  <blackbox>
    <mre_machine>
      <update_rule>square</update_rule>
      <mode>exploration</mode>
      <nb_runs>1275</nb_runs>
      <nb_steps>100</nb_steps>
      <problem>cart_pole_stabilization</problem>
      <save_run_logs>false</save_run_logs>
      <save_best_policy>false</save_best_policy>
      <mre>
        <mrefpf_conf>
          <nb_threads>40</nb_threads>
          <horizon>5</horizon>
          <discount>0.98</discount>
          <max_action_tiles>200</max_action_tiles>
          <reward_max>0</reward_max>
        </mrefpf_conf>
      </mre>
    </mre_machine>
  </blackbox>
</mre_experiment>
