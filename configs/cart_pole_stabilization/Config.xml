<Config>
  <nb_runs>100</nb_runs>
  <nb_steps>100</nb_steps>
  <problem>CartPoleStabilization</problem>
  <mre>
    <plan_period>-1</plan_period>
    <mrefpf_conf>
      <nb_threads>1</nb_threads>
      <x_dim>2</x_dim>
      <u_dim>1</u_dim>
      <x_limits>
        <v>-1.5708</v>
        <v>-2</v>
        <v>1.5708</v>
        <v>2</v>
      </x_limits>
      <u_limits>
        <v>-50</v>
        <v>50</v>
      </u_limits>
      <horizon>15</horizon>
      <discount>0.95</discount>
      <policy_samples>0</policy_samples>
      <max_action_tiles>200</max_action_tiles>
      <q_value_time>0</q_value_time>
      <policy_time>0</policy_time>
      <q_value_conf>
        <k>3</k>
        <n_min>2</n_min>
        <nb_trees>25</nb_trees>
        <nb_threads>1</nb_threads>
        <min_var>0.000001</min_var>
        <appr_type>PWC</appr_type>
        <val_min>-50</val_min>
        <val_max>0</val_max>
      </q_value_conf>
      <policy_conf>
        <k>2</k>
        <n_min>20</n_min>
        <nb_trees>25</nb_trees>
        <nb_threads>1</nb_threads>
        <min_var>0.0001</min_var>
        <appr_type>PWL</appr_type>
        <val_min>-15</val_min>
        <val_max>15</val_max>
      </policy_conf>
      <reward_max>0</reward_max>
      <filter_samples>false</filter_samples>
      <update_type>Alternative</update_type>
    </mrefpf_conf>
    <knownness_conf>
      <nb_trees>25</nb_trees>
      <tree_conf>
        <max_points>10</max_points>
        <type>Random</type>
      </tree_conf>
    </knownness_conf>
  </mre>
</Config>
