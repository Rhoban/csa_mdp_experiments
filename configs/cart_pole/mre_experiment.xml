<mre_experiment>
  <class_name>control</class_name>
  <mre_machine>
    <nb_runs>10</nb_runs>
    <nb_steps>100</nb_steps>
    <problem>CartPole</problem>
    <mre>
      <plan_period>-1</plan_period>
      <mrefpf_conf>
        <nb_threads>3</nb_threads>
        <horizon>20</horizon>
        <discount>0.98</discount>
        <max_action_tiles>200</max_action_tiles>
        <auto_parameters>true</auto_parameters>
        <reward_max>0</reward_max>
        <update_type>Alternative</update_type>
        <filter_samples>false</filter_samples>
      </mrefpf_conf>
      <knownness_conf>
        <nb_trees>25</nb_trees>
        <tree_conf>
          <max_points>10</max_points>
          <type>Random</type>
        </tree_conf>
      </knownness_conf>
    </mre>
    <control>
      <robot>cart_pole</robot>
      <frequency>10</frequency>
      <linear_sensors>
        <v>cart_joint</v>
      </linear_sensors>
      <angular_sensors>
        <v>axis1</v>
      </angular_sensors>
      <effectors>
        <v>cart</v>
      </effectors>
    </control>
  </mre_machine>
</mre_experiment>
