<Config>
  <nb_runs>100</nb_runs>
  <nb_steps>100</nb_steps>
  <problem>CartPole</problem>
  <control>
    <robot>cart_pole</robot>
    <frequency>10</frequency>
    <linear_sensors>
      <v>cart_joint</v>
    </linear_sensors>
    <angular_sensors>
      <v>axis1</v>
    </angular_sensors>
    <effectors>
      <v>cart</v>
    </effectors>
  </control>
  <mre>
    <plan_period>-1</plan_period>
    <mrefpf_conf>
      <nb_threads>1</nb_threads>
      <x_dim>4</x_dim>
      <u_dim>1</u_dim>
      <x_limits>
        <v>-1</v>
        <v>-5</v>
        <v>-3.1416</v>
        <v>-15</v>
        <v>1</v>
        <v>5</v>
        <v>3.1416</v>
        <v>15</v>
      </x_limits>
      <u_limits>
        <v>-20</v>
        <v>20</v>
      </u_limits>
      <horizon>20</horizon>
      <discount>0.98</discount>
      <policy_samples>0</policy_samples>
      <max_action_tiles>200</max_action_tiles>
      <q_value_time>0</q_value_time>
      <policy_time>0</policy_time>
      <q_value_conf>
        <k>5</k>
        <n_min>2</n_min>
        <nb_trees>25</nb_trees>
        <nb_threads>1</nb_threads>
        <min_var>0.000001</min_var>
        <appr_type>PWC</appr_type>
        <val_min>-50</val_min>
        <val_max>0</val_max>
      </q_value_conf>
      <policy_conf>
        <k>4</k>
        <n_min>40</n_min>
        <nb_trees>25</nb_trees>
        <nb_threads>1</nb_threads>
        <min_var>0.0001</min_var>
        <appr_type>PWL</appr_type>
        <val_min>-15</val_min>
        <val_max>15</val_max>
      </policy_conf>
      <reward_max>0</reward_max>
      <filter_samples>true</filter_samples>
      <update_type>Alternative</update_type>
    </mrefpf_conf>
    <knownness_conf>
      <nb_trees>25</nb_trees>
      <tree_conf>
        <max_points>10</max_points>
        <type>Random</type>
      </tree_conf>
    </knownness_conf>
  </mre>
</Config>
