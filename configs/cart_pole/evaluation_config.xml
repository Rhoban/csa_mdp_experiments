<mre_experiment>
  <control>
    <mre_machine>
      <mode>evaluation</mode>
      <policy>
        <forests_policy>
          <paths>
            <v>../policy_0.data</v>
          </paths>
        </forests_policy>
      </policy>
      <nb_runs>10</nb_runs>
      <nb_steps>100</nb_steps>
      <problem>cart_pole</problem>
      <control>
        <robot>cart_pole</robot>
        <frequency>25</frequency>
        <linear_sensors>
          <v>cart_joint</v>
        </linear_sensors>
        <angular_sensors>
          <v>axis1</v>
        </angular_sensors>
        <effectors>
          <v>cart</v>
        </effectors>
      </control>
    </mre_machine>
  </control>
</mre_experiment>
