<Config>
  <log_path>../logs.csv</log_path>
  <problem>CartPole</problem>
  <run_column>1</run_column>
  <step_column>2</step_column>
  <state_columns>
    <v>3</v>
    <v>4</v>
    <v>5</v>
    <v>6</v>
  </state_columns>
  <action_columns>
    <v>7</v>
  </action_columns>
  <fpf_conf>
    <nb_threads>1</nb_threads>
    <x_dim>4</x_dim>
    <u_dim>1</u_dim>
    <x_limits>
