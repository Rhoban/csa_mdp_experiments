<mre_experiment>
  <class_name>control</class_name>
  <mre_machine>
    <mode>evaluation</mode>
    <policy>
      <class_name>random</class_name>
    </policy>
    <nb_runs>10</nb_runs>
    <nb_steps>100</nb_steps>
    <problem>CartPole</problem>
    <control>
      <robot>cart_pole</robot>
      <frequency>25</frequency>
      <linear_sensors>
        <v>cart_joint</v>
      </linear_sensors>
      <angular_sensors>
        <v>axis1</v>
      </angular_sensors>
      <effectors>
        <v>cart</v>
      </effectors>
    </control>
  </mre_machine>
</mre_experiment>
