<mre_experiment>
  <blackbox>
    <mre_machine>
      <update_rule>square</update_rule>
      <mode>exploration</mode>
      <nb_runs>325</nb_runs>
      <nb_steps>25</nb_steps>
      <problem>
        <simulated_cart_pole>
          <learning_space>Angular</learning_space>
        </simulated_cart_pole>
      </problem>
      <mre>
        <mrefpf_conf>
          <nb_threads>3</nb_threads>
          <horizon>5</horizon>
          <discount>0.98</discount>
          <max_action_tiles>200</max_action_tiles>
          <reward_max>0</reward_max>
        </mrefpf_conf>
      </mre>
    </mre_machine>
  </blackbox>
</mre_experiment>
