<LearningMachine>
  <LearningMachineBlackBox>
    <problem>
      <simulated_cart_pole>
        <learning_space>Angular</learning_space>
      </simulated_cart_pole>
    </problem>
    <learner>
      <ModelBasedLearner>
        <model>
          <simulated_cart_pole>
            <learning_space>Angular</learning_space>
          </simulated_cart_pole>
        </model>
        <reward_predictor>
          <MonteCarloPredictor>
            <nb_steps>25</nb_steps>
            <nb_predictions>20</nb_predictions>
          </MonteCarloPredictor>
        </reward_predictor>
        <value_trainer>PWCForestTrainer</value_trainer>
        <action_optimizer>
          <BasicOptimizer>
            <trainer>PWCForestTrainer</trainer>
            <nb_simulations>5</nb_simulations>
            <nb_actions>25</nb_actions>
            <nb_additional_steps>0</nb_additional_steps>
          </BasicOptimizer>
        </action_optimizer>
        <policy_trainer>PWLForestTrainer</policy_trainer>
      </ModelBasedLearner>
    </learner>
    <save_best_policy>false</save_best_policy>
    <save_run_logs>false</save_run_logs>
    <save_details>false</save_details>
    <nb_threads>40</nb_threads>
    <nb_runs>1275</nb_runs>
    <nb_steps>25</nb_steps>
    <time_budget>600</time_budget>
    <discount>0.98</discount>
  </LearningMachineBlackBox>
</LearningMachine>
