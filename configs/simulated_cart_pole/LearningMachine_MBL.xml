<LearningMachine>
  <problem>
    <simulated_cart_pole>
      <learning_space>Angular</learning_space>
    </simulated_cart_pole>
  </problem>
  <learner>
    <ModelBasedLearner>
      <model>
        <simulated_cart_pole>
          <learning_space>Angular</learning_space>
        </simulated_cart_pole>
      </model>
      <reward_predictor>
        <MonteCarloPredictor>
          <nb_steps>25</nb_steps>
          <nb_predictions>200</nb_predictions>
        </MonteCarloPredictor>
      </reward_predictor>
      <value_trainer>PWCForestTrainer</value_trainer>
      <action_optimizer>
        <BasicOptimizer>
          <trainer>PWCForestTrainer</trainer>
          <nb_simulations>200</nb_simulations>
          <nb_actions>25</nb_actions>
          <nb_additional_steps>2</nb_additional_steps>
        </BasicOptimizer>
      </action_optimizer>
      <policy_trainer>PWCForestTrainer</policy_trainer>
    </ModelBasedLearner>
  </learner>
  <save_run_logs>false</save_run_logs>
  <nb_threads>3</nb_threads>
  <nb_runs>1275</nb_runs>
  <nb_steps>25</nb_steps>
  <time_budget>600</time_budget>
</LearningMachine>
