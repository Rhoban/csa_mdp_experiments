      <mode>exploration</mode>
      <nb_runs>55</nb_runs>
      <nb_steps>40</nb_steps>
      <problem>simulated_cart_pole</problem>
      <mre>
        <mrefpf_conf>
          <nb_threads>3</nb_threads>
          <horizon>5</horizon>
          <discount>1</discount>
          <max_action_tiles>200</max_action_tiles>
          <reward_max>0</reward_max>
        </mrefpf_conf>
      </mre>
    </mre_machine>
  </blackbox>
</mre_experiment>
