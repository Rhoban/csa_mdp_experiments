<LearningMachine>
  <LearningMachineBlackBox>
    <problem>
      <multi_kick_single_player>
        <kick_options>
          <v>
            <kick_model>
              <FullPowerKick>
                <kick_pow_rel_noise>0.1</kick_pow_rel_noise>
                <kick_direction_noise>0.3</kick_direction_noise>
                <kick_reward>-10</kick_reward>
                <kick_power>3</kick_power>
              </FullPowerKick>
            </kick_model>
            <approach_model/>
            <policy>
              <expert_approach>
                <type>polar</type>
              </expert_approach>
            </policy>
          </v>
        </kick_options>
      </multi_kick_single_player>
    </problem>
    <learner>
      <FakeLearner>
        <policy>
          <fa_policy>
            <path>../policy_tree.bin</path>
          </fa_policy>
        </policy>
      </FakeLearner>
    </learner>
    <nb_threads>1</nb_threads>
    <nb_runs>50</nb_runs>
    <nb_steps>50</nb_steps>
    <time_budget>36000</time_budget>
    <discount>1.0</discount>
  </LearningMachineBlackBox>
</LearningMachine>
