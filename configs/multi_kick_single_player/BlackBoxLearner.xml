<BlackBoxLearner>
  <PolicyMutationLearner>
    <problem>
      <multi_kick_single_player>
        <kick_options>
          <v>
            <kick_model>
              <FullPowerKick>
                <kick_pow_rel_noise>0.1</kick_pow_rel_noise>
                <kick_direction_noise>0.3</kick_direction_noise>
                <kick_reward>-10</kick_reward>
                <kick_power>3</kick_power>
              </FullPowerKick>
            </kick_model>
            <approach_model/>
            <policy>
              <expert_approach>
                <type>polar</type>
              </expert_approach>
            </policy>
          </v>
          <v>
            <kick_model>
              <CustomPowerKick>
                <kick_pow_rel_noise>0.1</kick_pow_rel_noise>
                <kick_direction_noise>0.3</kick_direction_noise>
                <kick_reward>-10</kick_reward>
                <min_kick_power>0.2</min_kick_power>
                <max_kick_power>1.5</max_kick_power>
              </CustomPowerKick>
            </kick_model>
            <approach_model/>
            <policy>
              <expert_approach>
                <type>polar</type>
              </expert_approach>
            </policy>
          </v>
        </kick_options>
      </multi_kick_single_player>
    </problem>
    <nb_threads>3</nb_threads>
    <trial_length>50</trial_length>
    <training_evaluations>40</training_evaluations>
    <training_evaluations_growth>0.2</training_evaluations_growth>
    <split_probability>0.1</split_probability>
    <nb_evaluation_trials>1000</nb_evaluation_trials>
    <evaluations_ratio>10</evaluations_ratio>
    <evaluations_growth>20</evaluations_growth>
    <avoid_growing_slopes>false</avoid_growing_slopes>
    <shared_initial_states>false</shared_initial_states>
    <time_budget>36000</time_budget>
    <discount>1.0</discount>
    <optimizer>
      <CompositeOptimizer>
        <names>
          <v>CMAES</v>
          <v>CE</v>
          <v>SA</v>
        </names>
        <optimizers>
          <v>
            <CMAESOptimizer>
              <nb_restarts>2</nb_restarts>
              <population_size>20</population_size>
              <max_history>10</max_history>
              <ftolerance>0.01</ftolerance>
              <elitism>0</elitism>
            </CMAESOptimizer>
          </v>
          <v>
            <CrossEntropy>
              <nb_generations>10</nb_generations>
            </CrossEntropy>
          </v>
          <v>
            <SimulatedAnnealing>
              <initial_temperature>2</initial_temperature>
            </SimulatedAnnealing>
          </v>
        </optimizers>
      </CompositeOptimizer>
    </optimizer>
  </PolicyMutationLearner>
</BlackBoxLearner>
