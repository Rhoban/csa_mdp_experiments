<BlackBoxLearner>
  <TreePolicyIteration>
    <problem>double_integrator</problem>
    <use_value_approximator>false</use_value_approximator>
    <policy_trainer>
      <AdaptativeTree>
        <reuse_samples>false</reuse_samples>
        <max_depth>5</max_depth>
        <verbosity>3</verbosity>
        <nb_generations>1</nb_generations>
        <nb_samples>50</nb_samples>
        <evaluation_trials>2</evaluation_trials>
        <narrow_linear_slope>false</narrow_linear_slope>
        <constant_uses_guess>true</constant_uses_guess>
        <linear_uses_guess>true</linear_uses_guess>
        <model_optimizer>
          <CrossEntropy>
            <nb_generations>20</nb_generations>
            <population_size>200</population_size>
            <best_set_size>20</best_set_size>
          </CrossEntropy>
        </model_optimizer>
      </AdaptativeTree>
    </policy_trainer>
    <memoryless_policy_trainer>true</memoryless_policy_trainer>
    <nb_threads>3</nb_threads>
    <trial_length>50</trial_length>
    <nb_evaluation_trials>50000</nb_evaluation_trials>
    <time_budget>72000</time_budget>
    <cv_ratio>100</cv_ratio>
    <discount>0.98</discount>
  </TreePolicyIteration>
</BlackBoxLearner>
