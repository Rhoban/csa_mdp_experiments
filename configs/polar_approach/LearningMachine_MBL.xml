<LearningMachine>
  <LearningMachineBlackBox>
    <problem>polar_approach</problem>
    <learner>
      <ModelBasedLearner>
        <model>polar_approach</model>
        <reward_predictor>
          <MonteCarloPredictor>
            <nb_steps>50</nb_steps>
            <nb_predictions>20</nb_predictions>
          </MonteCarloPredictor>
        </reward_predictor>
        <value_trainer>PWCForestTrainer</value_trainer>
        <action_optimizer>
          <BasicOptimizer>
            <trainer>PWCForestTrainer</trainer>
            <nb_simulations>10</nb_simulations>
            <nb_actions>100</nb_actions>
            <nb_additional_steps>0</nb_additional_steps>
          </BasicOptimizer>
        </action_optimizer>
        <policy_trainer>PWLForestTrainer</policy_trainer>
      </ModelBasedLearner>
    </learner>
    <seed_path>../seed/run_logs.csv</seed_path>
    <save_run_logs>false</save_run_logs>
    <nb_threads>3</nb_threads>
    <nb_runs>55</nb_runs>
    <nb_steps>50</nb_steps>
    <time_budget>600</time_budget>
  </LearningMachineBlackBox>
</LearningMachine>
