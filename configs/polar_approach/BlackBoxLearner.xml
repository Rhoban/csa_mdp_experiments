<BlackBoxLearner>
  <PolicyMutationLearner>
    <problem>
      <polar_approach>
        <odometry_coefficients>
          <v>0.0093</v>
          <v>-0.0026</v>
          <v>0.0002</v>
          <v>1.30</v>
          <v>-0.075</v>
          <v>-0.35</v>
          <v>-0.17</v>
          <v>1.47</v>
          <v>0.05</v>
          <v>0.001</v>
          <v>0.101</v>
          <v>0.909</v>
        </odometry_coefficients>
        <max_step_theta>0.2</max_step_theta>
      </polar_approach>
    </problem>
    <policy>
      <expert_approach>
        <type>polar</type>
      </expert_approach>
    </policy>
    <nb_threads>20</nb_threads>
    <trial_length>50</trial_length>
    <training_evaluations>40</training_evaluations>
    <training_evaluations_growth>1</training_evaluations_growth>
    <split_probability>0.1</split_probability>
    <change_action_probability>0.2</change_action_probability>
    <nb_evaluation_trials>5000</nb_evaluation_trials>
    <evaluations_ratio>10</evaluations_ratio>
    <evaluations_growth>50</evaluations_growth>
    <avoid_growing_slopes>false</avoid_growing_slopes>
    <shared_initial_states>false</shared_initial_states>
    <time_budget>7200</time_budget>
    <discount>1.0</discount>
    <optimizer>
      <CompositeOptimizer>
        <names>
          <v>CMAES</v>
          <v>CE</v>
          <v>SA</v>
        </names>
        <optimizers>
          <v>
            <CMAESOptimizer>
              <nb_restarts>2</nb_restarts>
              <population_size>20</population_size>
              <max_history>10</max_history>
              <ftolerance>0.01</ftolerance>
              <elitism>0</elitism>
            </CMAESOptimizer>
          </v>
          <v>
            <CrossEntropy>
              <nb_generations>10</nb_generations>
            </CrossEntropy>
          </v>
          <v>
            <SimulatedAnnealing>
              <initial_temperature>2</initial_temperature>
            </SimulatedAnnealing>
          </v>
        </optimizers>
      </CompositeOptimizer>
    </optimizer>
  </PolicyMutationLearner>
</BlackBoxLearner>
