<LearningMachine>
  <problem>polar_approach</problem>
  <learner>
    <MRE>
      <mrefpf_conf>
        <horizon>5</horizon>
        <discount>0.98</discount>
        <max_action_tiles>2000</max_action_tiles>
        <reward_max>0</reward_max>
      </mrefpf_conf>
    </MRE>
  </learner>
  <seed_path>../seed/run_logs.csv</seed_path>
  <save_run_logs>false</save_run_logs>
  <nb_threads>3</nb_threads>
  <nb_runs>55</nb_runs>
  <nb_steps>50</nb_steps>
  <time_budget>600</time_budget>
</LearningMachine>
