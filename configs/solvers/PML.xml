<BlackBoxLearner>
  <PolicyMutationLearner>
    <problem_path>problem.xml</problem_path>
    <nb_threads>1</nb_threads>
    <trial_length>200</trial_length>
    <training_evaluations>10</training_evaluations>
    <nb_evaluation_trials>200</nb_evaluation_trials>
    <evaluations_ratio>10</evaluations_ratio>
    <split_probability>0.1</split_probability>
    <time_budget>3600</time_budget>
    <discount>1.0</discount>
    <optimizer>
      <CrossEntropy>
        <nb_generations>10</nb_generations>
      </CrossEntropy>
    </optimizer>
  </PolicyMutationLearner>
</BlackBoxLearner>
